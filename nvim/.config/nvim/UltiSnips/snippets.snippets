# trick, snippets for snippet files
extends text

snippet snip "Ultisnips snippet definition" b
`!p snip.rv = "snippet"` ${1:trigger} "${2:Description}" ${3:b}
$0
`!p snip.rv = "endsnippet"`
endsnippet

snippet gp "Ultsnips global python" b
global !p
$0
endglobal
endsnippet

snippet gpmod "Call nonomain py module" b
global !p

import os
mod_name = os.path.expandvars(os.environ["XDG_CONFIG_HOME"])
mod_name += '/nvim/UltiSnips/nonomain.py'
exec(open(mod_name).read())

endglobal
endsnippet
